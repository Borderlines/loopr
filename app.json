{
  "name":"l8pr",
  "scripts":{
    "postdeploy": "python3 manage.py migrate"
  },
  "env":{
    "DJANGO_SETTINGS_MODULE:":{
      "required":true
    },
    "IFRAMELY_API_KEY:":{
      "required":true
    },
    "GOOGLE_API_KEY":{
      "required":true
    },
    "TWITTER_ACCESS_TOKEN":{
      "required":true
    },
    "TWITTER_ACCESS_TOKEN_SECRET":{
      "required":true
    },
    "TWITTER_CONSUMER_KEY":{
      "required":true
    },
    "TWITTER_CONSUMER_SECRET":{
      "required":true
    }
  },
  "addons":[
    "heroku-postgresql",
    "bonsai"
  ],
  "buildpacks": [
    {
      "url": "https://github.com/ddollar/heroku-buildpack-multi.git"
    }
  ]
}
