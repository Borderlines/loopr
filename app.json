{
  "name":"l8pr",
  "scripts":{
    "postdeploy": "python3 manage.py migrate"
  },
  "env":{
    "GOOGLE_API_KEY":{
      "required":true
    },
    "TWITTER_ACCESS_TOKEN":{
      "required":true
    },
    "TWITTER_ACCESS_TOKEN_SECRET":{
      "required":true
    },
    "TWITTER_CONSUMER_KEY":{
      "required":true
    },
    "TWITTER_CONSUMER_SECRET":{
      "required":true
    }
  },
  "addons":[
    "heroku-postgresql",
    "bonsai"
  ],
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-python"
    }
  ]
}
